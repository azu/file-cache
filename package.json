{
  "private": true,
  "name": "file-cache-root",
  "infra": "monorepo",
  "scripts": {
    "build": "moon run :build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "moon": "moon --color --log trace",
    "prepare": "git config --local core.hooksPath .githooks",
    "release": "npm run build && npm publish --workspaces",
    "test": "moon run :test",
    "versionup:major": "npm version major --workspaces --include-workspace-root",
    "versionup:minor": "npm version minor --workspaces --include-workspace-root",
    "versionup:patch": "npm version patch --workspaces --include-workspace-root"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@moonrepo/cli": "^0.22.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "eslint": "^8.32.0",
    "eslint-config-beemo": "^2.1.2",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "packemon": "^2.4.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "packageManager": "npm@8.19.2",
  "engines": {
    "node": "18.12.1"
  },
  "version": "1.1.3",
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  }
}
