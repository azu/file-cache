{
  "private": true,
  "name": "moon-examples",
  "infra": "monorepo",
  "scripts": {
    "moon": "moon --color --log trace",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@moonrepo/cli": "^0.6.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "eslint": "^8.16.0",
    "eslint-config-beemo": "^2.1.1",
    "jest": "^28.1.0",
    "jest-preset-beemo": "^2.2.0",
    "lint-staged": "^13.0.3",
    "packemon": "^2.3.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  }
}
