{
  "private": true,
  "name": "file-cache-root",
  "infra": "monorepo",
  "scripts": {
    "build": "moon run :build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "moon": "moon --color --log trace",
    "prepare": "git config --local core.hooksPath .githooks",
    "release": "npm run build && npm publish --workspaces",
    "test": "moon run :test",
    "test:example": "moon run :test",
    "versionup:major": "npm version major --workspaces --include-workspace-root",
    "versionup:minor": "npm version minor --workspaces --include-workspace-root",
    "versionup:patch": "npm version patch --workspaces --include-workspace-root"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@moonrepo/cli": "^1.19.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "eslint-config-beemo": "^2.1.2",
    "lint-staged": "^15.2.0",
    "mocha": "^10.2.0",
    "packemon": "^3.2.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "packageManager": "npm@8.19.2",
  "engines": {
    "node": "18.16.1"
  },
  "version": "1.1.4",
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  }
}
