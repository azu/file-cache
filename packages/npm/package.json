{
  "name": "@file-cache/npm",
  "version": "1.0.0",
  "description": "npm util for @file-cache",
  "keywords": [
    "node",
    "cache",
    "npm"
  ],
  "type": "module",
  "author": "azu",
  "license": "MIT",
  "repository": "https://github.com/azu/file-cache",
  "packemon": [
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "cjs"
    },
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "mjs"
    }
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "types": "./dts/index.d.ts",
  "main": "./mjs/index.mjs",
  "dependencies": {
    "resolve": "^1.22.1"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "ts-node": "^10.8.2",
    "@types/node": "^18.0.3",
    "version-1.0.0": "1.0.0"
  },
  "scripts": {
    "test": "mocha \"tests/**/*.test.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": {
        "import": "./mjs/*.mjs",
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./mjs/index.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
