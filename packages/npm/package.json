{
  "name": "@file-cache/npm",
  "version": "2.0.0",
  "description": "npm util for @file-cache",
  "keywords": [
    "node",
    "cache",
    "npm"
  ],
  "type": "module",
  "author": "azu",
  "license": "MIT",
  "repository": "https://github.com/azu/file-cache",
  "packemon": [
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "cjs"
    },
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "mjs"
    }
  ],
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=6.14.0"
  },
  "types": "./mjs/index.d.ts",
  "main": "./mjs/index.mjs",
  "dependencies": {
    "resolve": "^1.22.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.2",
    "version-1.0.0": "1.0.0"
  },
  "scripts": {
    "test": "mocha \"tests/**/*.test.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./mjs/*.d.ts",
      "import": "./mjs/*.mjs",
      "default": "./cjs/*.cjs"
    },
    ".": {
      "types": "./mjs/index.d.ts",
      "import": "./mjs/index.mjs",
      "default": "./cjs/index.cjs"
    }
  },
  "files": [
    "cjs/**/*",
    "mjs/**/*",
    "src/**/*"
  ]
}
