{
  "name": "@file-cache/core",
  "version": "1.1.4",
  "description": "A cache for file metadata or file content.",
  "keywords": [
    "node",
    "cache"
  ],
  "type": "module",
  "author": "azu",
  "license": "MIT",
  "repository": "https://github.com/azu/file-cache",
  "packemon": [
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "cjs"
    },
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "mjs"
    }
  ],
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=6.14.0"
  },
  "types": "./mjs/index.d.ts",
  "main": "./mjs/index.mjs",
  "dependencies": {
    "find-cache-dir": "^5.0.0"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "ts-node": "^10.8.2",
    "@types/node": "^20.4.2"
  },
  "scripts": {
    "test": "mocha \"tests/**/*.test.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "node": {
        "types": "./mjs/*.d.ts",
        "import": "./mjs/*.mjs",
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "node": {
        "types": "./mjs/index.d.ts",
        "import": "./mjs/index.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  },
  "files": [
    "cjs/**/*",
    "mjs/**/*",
    "src/**/*"
  ]
}
