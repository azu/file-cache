{
  "name": "@company/node-package",
  "version": "0.0.0",
  "description": "A Node.js package.",
  "keywords": [
    "node",
    "lib"
  ],
  "author": "moon",
  "license": "MIT",
  "repository": "https://github.com/moonrepo/examples",
  "packemon": [
    {
      "inputs": {
        "index": "src/index.ts"
      },
      "platform": "node"
    },
    {
      "inputs": {
        "web": "src/index.ts"
      },
      "platform": "browser",
      "support": "current"
    },
    {
      "inputs": {
        "node": "src/index.ts"
      },
      "format": "mjs",
      "platform": "node",
      "support": "experimental"
    }
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "types": "./dts/index.d.ts",
  "main": "./mjs/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./mjs/index.mjs"
      }
    },
    "./node": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./mjs/node.mjs"
      }
    },
    "./web": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/web.js",
        "import": "./esm/web.js"
      },
      "default": "./lib/web.js"
    }
  }
}
